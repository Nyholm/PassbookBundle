{
    "name":        "eo/passbook-bundle",
    "type":        "symfony-bundle",
    "description": "iOS Passbook for Symfony2",
    "keywords":    ["passbook", "ios", "pass", "mobile", "wallet", "coupons", "tickets"],
    "homepage":    "https://github.com/eymengunay/passbook-bundle",
    "license":     "MIT",
    "authors": [
        {
            "name": "Eymen Gunay",
            "email": "eymen@egunay.com"
        }
    ],
    "require": {
        "symfony/symfony": ">=2.1,<2.3-dev",
        "eo/passbook": "dev-master"
    },
    "target-dir": "Eo/PassbookBundle",
    "autoload": {
        "psr-0": {
            "Eo\\PassbookBundle": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
